    public void saveFileAs() {
        if (readonlyResp == ReadResp.NO_CHARSET){
            QMessageBox.Critical("注意","未选择有效的编码,文件将锁定为只读,保存或者编辑将使文件损坏.",QMessageBox.Ok,QMessageBox.Ok);
            return;
        }
        
