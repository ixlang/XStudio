        }
        return false;
    }
    public bool isKeyWord(String word){
        if (word == nilptr || word.length() == 0){
            return false;
        }
        for (int i = 0, c = word.length(); i < c; i++){
            if (isInWord(word.charAt(i),i == 0) == false){
                return false;
            }
        }
        return true;
    }
    
