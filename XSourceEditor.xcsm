    public static bool closeAllWindows() {

        Map.Iterator<String, DocumentView> iterator = editorMgr.iterator();

        while (iterator.hasNext()) {
            DocumentView wnd = iterator.getValue();

            if (wnd != nilptr && wnd.requestClose() == false) {
                return false;
            }

            iterator.next();
        }

        return true;
    }

