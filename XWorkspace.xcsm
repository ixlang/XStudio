    public void openFile(String file)
    {
        if (file != nilptr && file.length() > 0) {
            String exts = file.findExtension();

            if (exts == nilptr || exts.equals(".xprj") == false) {
                openTextFile(file);
            } else {
                loadProject(file);
            }
            updateDocumentStatus();
        }
    }

