            items[pos++] = iterator.getValue().name;
            iterator.next();
        }

        cfgSel.addItems(items);
    }

    public bool requestQuit()
    {
        bool bclose = true;
        if (currentProject != nilptr) {
            if (currentProject.isModified()) {
                int rt = QXMessageBox.Question("提示","项目[" + currentProject.getName() + "]已发生更改, 是否保存?",  QXMessageBox.Cancel | QXMessageBox.No | QXMessageBox.Save, QXMessageBox.Save);
                if (rt == QXMessageBox.Save) {
                    saveProject();
                    bclose = true;
                } else if (rt == QXMessageBox.No) {
                    bclose = true;
                } else if (rt == QXMessageBox.Cancel) {
                    bclose = false;
                }
            }
            if (bclose) {
                bclose = closeProject();
            }
        }
        
        if (bclose){
            if (DownloadCenter.hasDownload()){
                int rt = QXMessageBox.Question("提示","有下载任务在进行中,是否继续退出?",  QXMessageBox.Yes | QXMessageBox.No , QXMessageBox.No);
                if (rt == QXMessageBox.Yes){
                    DownloadCenter.stopAlldownload();
                }else{
                    bclose = false;
                    StatusBarMgr.showDLMgr();
                }
            }
        }
        return bclose;
    }
    public void openOutput()
    {
        if (currentProject != nilptr) {
            String uri = nilptr;
            IXPlugin ixplugin = currentProject.getProjectPlugin();
            
            if (ixplugin != nilptr){
                IProjectPlugin ipp = ixplugin.getProjectPlugin();
                if (ipp != nilptr){
                    uri = ipp.getTargetPath(currentProject);
                }
            }

            if (uri == nilptr){
                uri = currentProject.getOutputDir();
            }else{
                uri = uri.findVolumePath();
            }
            
            if (uri != nilptr) {
                QXCore.openLocal(String.formatPath(uri,false));
            }
        }
    }
