        public static class SourceContentImpl : SourceContent{
            String txtContent = nilptr;
            String charset = nilptr;
            String path = nilptr;
            bool bEditing = false;
            
            public SourceContentImpl(XSourceEditor wnd){
                txtContent = wnd.getTextContent();
                charset = wnd.defaultCharset;
                path = wnd.filePath;
                bEditing = true;
            }
            
            public SourceContentImpl(String file){
                txtContent = readFileUTF8(file);
                path = file;
                if (txtContent == nilptr){
                    throw new IllegalArgumentException();
                }
            }
            
            public String readFileUTF8(String file){
                long hfile = _system_.openFile(file,"r");
                if (hfile != 0 ){
                    long fl = _system_.fileLength(hfile);
                    byte [] data = new byte [fl];
                    _system_.readFile(hfile,data,0,fl);
                    _system_.closeFile(hfile);
                    
                    charset = XSourceEditor.getCharsetCache(file);
                    
                    if (charset == nilptr){
                        charset = String.detectCharset(data,0,fl);
                    }
                    if (charset == nilptr){
                        return new String(data);
                    }
                    switch(charset){
                        case "ASCII":
                        case "UTF-8":
                        case "UTF8":
                        return new String(data);
                        break;
                        
                        default:
                        return new String(data, 0, fl, charset);
                        break;
                    }
                }
                return nilptr;
            }
            
            public String getContent()override{
                return txtContent;
            }
            
            public String getPath()override{
                return path;
            }
            
            public bool isEditing()override{
                return bEditing;
            }
            
            public String getCharset()override{
                return charset;
            }
        };
        
        public SourceContent getSourceContent(String sourcefile)override{
            String _content = nilptr;
            XSourceEditor wnd = XSourceEditor.findFileWindow(sourcefile);
            if (wnd != nilptr) {
                 return new SourceContentImpl(wnd);
            }else{
                try{
                    return new SourceContentImpl(sourcefile);
                }catch(Exception e){
                    
                }
            }
            return nilptr;
        }
        
        
    };

