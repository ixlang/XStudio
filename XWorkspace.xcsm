                currentProject.save();
            }
            for (int i =0, c = currentProject.getSourceFileCount(); i < c; i++) {
                String file = currentProject.getSourcePath(i);
                if (XSourceEditor.findSaveFile(file)) {
                    QxApp.statusBar.showStatusMessage(file + "已保存.");
