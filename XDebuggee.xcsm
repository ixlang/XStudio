			case SwitchThread:
            
            break;
            
			case QueryFrame:
				onQueryFrame(tid, json);
            break;
            
			case QueryMemoryInfo:
				XMemoryWnd.memoryWnd.showInfo(json);
            break;
            
			case GC:
            
            break;
            
            case Log:
				XLogcatWnd.sendLogcat(json);
            break;
            case WatchObject:
				//_system_.consoleWrite(json.toString(true));
                WatchWnd.watchesWnd.showInfo(json);
            break;
            case GCInfo:
            if (Setting.outputGCStat() == false){
                break;
            }
            case Debug:
				String message = json.getString("message");
                if (message != nilptr){
                    if (_system_.getPlatformId() == 0){
                        byte [] data = message.getBytes();
                        try{
                            message = new String(data, 0, data.length, "CP936");
                        }catch(Exception e){
                            message = new String(data, 0, data.length);
                        }
                    }
                    XWndOutput.Output(message + "\n", 1);
                }
            break;
            
            case Active:
				active();
            break;
            
            case QueryObject:
				onQueryObject(json);
            break;
            
            case GcDump:
				onDumpObjects(json);
            break;
            
            case QueryByName:
				onQueryByName(json);
            break;
            
            case ModuleLoaded:
				onLoadModule(json);
            break;
            
            case LookupMemory:
				onLookupMemory(json);
            break;
            
            case DumpMemory:
				onDumpMemory(json);
            break;
            
            case MessageBox:
                onMessageBox(json);
            break;
            
            default:
            case DBGExit:
                return false;
            break;
            
        }
        return true;
    }
};