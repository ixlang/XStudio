            onInterrupt(tid, json);
            break;
            
			case Continue:
            
            break;
            
			case StepIn:
            
            break;
            
			case StepOver:
            
            break;
            
			case StepOut :
            
            break;
            
			case SetBreakPoint:
				onSetBreakPoint(json);
            break;
            
			case SetMemoryBreakPoint:
            
            break;
            
			case SetFunctionBreakPoint:
            
